{
    "extends": "airbnb",
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "linebreak-style":"off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "jsx-a11y/anchor-is-valid": "off",
        "jsx-a11y/label-has-for": "off"
    }
}